{
  "name": "reactnodefullstackapp",
  "version": "0.0.1",
  "description": "Welcome to a full stack app of react and node",
  "main": "app.js",
  "type": "module",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "postinstall": "cd client && npm install && cd ../graphql && npm install",
    "launch": "cd graphql && npm run launchDocker",
    "migrate": "cd graphql && npm run migrate",
    "seed": "node seed/loadAPI.js",
    "seed:game": "cd graphql && npm run seed:game",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "build": "npm install && cd client && npm install && npm run build",
    "start-dev": "concurrently --kill-others \"nodemon app.js\" \"cd graphql && npm run dev\" \"cd client && npm run start\"",
    "buildDocker": "docker build .",
    "start": "node app.js"
  },
  "author": "Aubrey Jones",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aubreyjonescreatives/react-node-full-stack-app.git"
  },
  "bugs": {
    "url": "https://github.com/aubreyjonescreatives/react-node-full-stack-app/issues"
  },
  "homepage": "https://github.com/aubreyjonescreatives/react-node-full-stack-app#readme",
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^5.11.19",
    "path": "^0.12.7",
    "react-bootstrap": "^1.5.2",
    "react-router-dom": "^5.2.0",
    "script": "file:graphql"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
